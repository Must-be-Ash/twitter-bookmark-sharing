generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @default(auto()) @map("_id") @test.ObjectId
  name          String?
  email         String   @unique
  emailVerified DateTime?
  image         String?
  username      String   @unique

  // Add any other fields you need
}
